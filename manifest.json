{
  "manifest_version": 2,
  "name": "Save Old Version",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon16.png",
      "128": "images/icon16.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

